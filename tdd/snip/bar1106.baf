		CreateCreatureObject("cavdrow2",Player1,0,0,0) // Drow
		CreateCreatureObject("cavdrow3",Player1,0,0,0) // Drow
		CreateVisualEffect("spsummon",[589.1189])
		CreateCreature("jarlcave",[589.1189],4) // Jarlaxle
