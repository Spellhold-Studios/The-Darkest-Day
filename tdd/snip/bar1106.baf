IF
	Global("jarlaxe1home","GLOBAL",3)
THEN
	RESPONSE #100
		Explore()
		StartCutSceneMode()
		StartCutScene("artycave")
		SetGlobal("jarlaxe1home","GLOBAL",4)
END

IF
	Global("caveartjarl","GLOBAL",1)
	InParty("jaheira")
THEN
